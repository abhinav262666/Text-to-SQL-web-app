import sqlite3

# Create a new SQLite database (or connect to an existing one)
conn = sqlite3.connect('your_database.db')

# Create a cursor object
cursor = conn.cursor()

# Create a table called "users" with columns: user_id, session_id, name, and age
cursor.execute('''
    CREATE TABLE IF NOT EXISTS users (
        user_id INTEGER PRIMARY KEY AUTOINCREMENT,
        session_id TEXT,
        name TEXT,
        age INTEGER
    )
''')

# Insert some sample data into the "users" table
cursor.execute('''
    INSERT INTO users (session_id, name, age)
    VALUES
    ('session_123', 'Alice', 25),
    ('session_456', 'Bob', 30),
    ('session_789', 'Charlie', 25)
''')

# Create a table called "products" with columns: product_id, product_name, and price
cursor.execute('''
    CREATE TABLE IF NOT EXISTS products (
        product_id INTEGER PRIMARY KEY AUTOINCREMENT,
        product_name TEXT,
        price REAL
    )
''')

# Insert some sample data into the "products" table
cursor.execute('''
    INSERT INTO products (product_name, price)
    VALUES
    ('Laptop', 1500),
    ('Phone', 800),
    ('Tablet', 600),
    ('Monitor', 300),
    ('Keyboard', 50)
''')

# Create a table called "orders" with columns: order_id, user_id, and product_id
cursor.execute('''
    CREATE TABLE IF NOT EXISTS orders (
        order_id INTEGER PRIMARY KEY AUTOINCREMENT,
        user_id INTEGER,
        product_id INTEGER,
        quantity INTEGER,
        FOREIGN KEY(user_id) REFERENCES users(user_id),
        FOREIGN KEY(product_id) REFERENCES products(product_id)
    )
''')

# Insert some sample data into the "orders" table
cursor.execute('''
    INSERT INTO orders (user_id, product_id, quantity)
    VALUES
    (1, 1, 2),
    (2, 2, 1),
    (3, 3, 3),
    (1, 4, 1),
    (2, 5, 4)
''')

# Create a table called "employees" with columns: employee_id, name, department, and salary
cursor.execute('''
    CREATE TABLE IF NOT EXISTS employees (
        employee_id INTEGER PRIMARY KEY AUTOINCREMENT,
        name TEXT,
        department TEXT,
        salary REAL
    )
''')

# Insert some sample data into the "employees" table
cursor.execute('''
    INSERT INTO employees (name, department, salary)
    VALUES
    ('John', 'Sales', 50000),
    ('Sarah', 'HR', 60000),
    ('Tom', 'IT', 55000),
    ('Anna', 'Marketing', 65000),
    ('Paul', 'Finance', 70000)
''')

# Create a table called "departments" with columns: department_id, department_name, and location
cursor.execute('''
    CREATE TABLE IF NOT EXISTS departments (
        department_id INTEGER PRIMARY KEY AUTOINCREMENT,
        department_name TEXT,
        location TEXT
    )
''')

# Insert some sample data into the "departments" table
cursor.execute('''
    INSERT INTO departments (department_name, location)
    VALUES
    ('Sales', 'New York'),
    ('HR', 'San Francisco'),
    ('IT', 'Chicago'),
    ('Marketing', 'Los Angeles'),
    ('Finance', 'Boston')
''')

# Commit the transaction
conn.commit()

# Close the connection
conn.close()

print("SQLite database and tables created successfully.")
